<UserControl x:Class="FourierCircles.GraphicOfPoints"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FourierCircles"
             mc:Ignorable="d" 
             d:DesignHeight="40" d:DesignWidth="80">
    <Canvas x:Name="GraphicCanvas" Background="LightSalmon">
        <Line x:Name="ReferenceLine" X1="0" Y1="0" X2="200" Y2="0" StrokeDashArray="2" Stroke="Gray" StrokeThickness="2"/>
    </Canvas>
</UserControl>
